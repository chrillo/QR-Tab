{
  "name": "QR Tab",
  "version": "1.0",
  "manifest_version": 1,
  "description": "Creates QR code for the URL of the current Tab",
  "browser_action": {
    "default_icon": "icon_18.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "64": "icon_64.png",
    "128": "icon_128.png",
    "256": "icon_256.png"
  },
  "permissions": ["tabs"]
}